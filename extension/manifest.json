{
  "manifest_version": 3,
  "name": "TailTube",
  "version": "1.2",
  "permissions": ["activeTab"],
  "host_permissions": [
    "http://{TAILSCALE IP HERE}/*",
    "https://sponsor.ajay.app/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
